{
  "name": "profinder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "pnpm run lint:check",
    "lint:check": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "format": "pnpm run format:fix",
    "format:fix": "pretty-quick --staged --pattern \"src/**/*.{js,jsx,ts,tsx}\" --ignore-path .gitignore",
    "format:check": "pretty-quick --check --pattern \"src/**/*.{js,jsx,ts,tsx}\" --ignore-path .gitignore"
  },
  "dependencies": {
    "@headlessui/react": "1.7.15",
    "axios": "1.4.0",
    "clsx": "1.2.1",
    "nprogress": "0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "4.9.0",
    "react-intl": "6.4.2",
    "sonner": "0.4.0",
    "zustand": "4.3.8"
  },
  "devDependencies": {
    "@types/nprogress": "0.2.0",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "10.4.14",
    "eslint": "^8.42.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "husky": "8.0.3",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "tailwindcss": "3.3.2",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "4.2.0"
  }
}
